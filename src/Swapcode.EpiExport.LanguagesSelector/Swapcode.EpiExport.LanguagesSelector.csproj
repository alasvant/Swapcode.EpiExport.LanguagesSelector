    <Copyright>Antti Alasvuo Â©2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Export,Languages</PackageTags>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/alasvant/Swapcode.EpiExport.LanguagesSelector.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/alasvant/Swapcode.EpiExport.LanguagesSelector</PackageProjectUrl>
    <Version>1.1.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPiServer.CMS.AspNet" Version="[11.1.0,12.0)" />
  </ItemGroup>

</Project>
